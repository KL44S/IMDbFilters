{
  "name": "IMDb Filters",
  "manifest_version": 2,
  "version": "1.3.0",
  "short_name": "IMDb Filters",
  "icons": {
    "48": "images/imdb-sm.png",
    "128": "images/imdb-lg.png"
  },
   "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "description": "Extensión para filtrar películas y series en IMDb",
   "content_scripts": [{
        "js": ["contentScript.js"],
        "matches": ["http://www.imdb.com/*"]
    }],
    "web_accessible_resources": ["imdbFilter.js"]
}